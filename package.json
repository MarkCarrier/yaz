{
  "name": "yaz",
  "version": "0.0.1",
  "description": "A tool for writing and sharing text documents",
  "main": "server/start.js",
  "scripts": {
    "build": "craco build",
    "dev": "nodemon -r dotenv/config server/start.js",
    "dev2": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "HOST=0.0.0.0 PORT=4410 DANGEROUSLY_DISABLE_HOST_CHECK=true BROWSER=none craco start",
    "dev:server": "nodemon -r dotenv/config server/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/MarkCarrier/yaz.git"
  },
  "keywords": [
    "markdown",
    "text",
    "share",
    "publish",
    "write"
  ],
  "author": "Mark Carrier <markcarrier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkCarrier/yaz/issues"
  },
  "dependencies": {
    "@babel/core": "^7.14.2",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.13.16",
    "aws-sdk": "^2.884.0",
    "axios": "^0.21.1",
    "bs58": "^4.0.1",
    "chardet": "^1.3.0",
    "core-js": "^3.14.0",
    "dotenv": "^8.2.0",
    "front-matter": "^4.0.2",
    "github-api": "^3.4.0",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-send": "^5.0.1",
    "koa-session": "^6.2.0",
    "luxon": "^1.27.0",
    "marked": "^2.0.3",
    "nanoid": "^3.1.23",
    "nodegit": "^0.27.0",
    "nodemon": "^2.0.7",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "/app/",
  "proxy": "http://localhost:4411",
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "autoprefixer": "^9.8.6",
    "concurrently": "^5.1.0",
    "postcss": "^7.0.35",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2"
  }
}
